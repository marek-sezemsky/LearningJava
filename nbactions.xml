<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dfile.encoding=UTF-8 --module-path g:\javafx-sdk-15.0.1\lib  --add-modules javafx.controls,javafx.fxml,javafx.graphics -classpath %classpath org.example.sezemsky.HelloFX</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address} -Dfile.encoding=UTF-8 --module-path g:\javafx-sdk-15.0.1\lib  --add-modules javafx.controls,javafx.fxml,javafx.graphics -classpath %classpath org.example.sezemsky.HelloFX</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dfile.encoding=UTF-8 --module-path g:\javafx-sdk-15.0.1\lib  --add-modules javafx.controls,javafx.fxml,javafx.graphics -classpath %classpath org.example.sezemsky.HelloFX</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-exec:java</actionName>
            <displayName>exec:java</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
                <goal>exec:java</goal>
            </goals>
        </action>
    </actions>
